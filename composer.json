{
  "name": "mll-lab/laravel-query-log",
  "description": "Log database queries to an output channel of your choice",
  "type": "library",
  "keywords": ["laravel", "php", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.2 || ^8",
    "illuminate/contracts": "5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8"
  },
  "require-dev": {
    "orchestra/database": "3.6.* || 3.7.* || 3.8.* || 3.9.* || ^4 || ^5 || ^6",
    "orchestra/testbench": "3.6.* || 3.7.* || 3.8.* || 3.9.* || ^4 || ^5 || ^6",
    "nunomaduro/larastan": "^0.4 || ^0.5"
  },
  "autoload": {
    "psr-4": {
      "MLL\\LaravelQueryLog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\LaravelQueryLog\\LaravelQueryLogServiceProvider"
      ]
    }
  }
}
